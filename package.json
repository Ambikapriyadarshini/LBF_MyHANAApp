{
  "name": "MyHANAApp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7.9",
    "@sap/cds-common-content": "^2.1.0",
    "@sap/cds-hana": "^2",
    "@sap/hdi-deploy": "^5.2.3",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7"
  },
  "scripts": {
    "start": "cds-serve",
    "build": "cds build"
  },
  "cds": {
    "requires": {
      "auth": "mocked",
      "db": "hana"
    },
    "build": {
      "target": "gen",
      "tasks": [
        {
          "for": "hana",
          "src": "db",
          "dest": "db",
          "options": {
            "model": ["db"]
          }
        },
        {
          "for": "nodejs",
          "src": "srv",
          "dest": "srv",
        "options": {
          "model": ["srv", "db"]
        }
        }
      ]
    }
  }
}
